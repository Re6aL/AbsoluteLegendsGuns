<?xml version="1.0" encoding="utf-8"?>
<Defs>

  	<!-- =========== Deagle sounds ============ -->

  <SoundDef>
    <defName>Shot_AL_Deagle</defName>  
    <context>MapOnly</context>
    <eventNames />  
    <maxSimultaneous>1</maxSimultaneous>  
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Pistol/Shot_AL_Deagle</clipPath>
          </li>
        </grains>      
        <pitchRange>
          <min>0.936413</min>        
          <max>1.042391</max>
        </pitchRange>               
        <sustainLoop>False</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>

  <SoundDef>
    <defName>Interact_AL_Deagle</defName>  
